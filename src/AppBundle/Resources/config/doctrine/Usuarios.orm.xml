<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Usuarios" table="usuarios">
    <unique-constraints>
      <unique-constraint name="nombre_usuario" columns="nombre_usuario"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombreUsuario" type="string" column="nombre_usuario" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="contrasenaUsuario" type="string" column="contrasena_usuario" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="estadoUsuario" type="integer" column="estado_usuario" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
